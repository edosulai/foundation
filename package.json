{
  "name": "foundation.edosulai.github.io",
  "version": "0.1.0",
  "scripts": {
    "dev": "modern dev",
    "start": "modern start",
    "serve": "modern serve",
    "new": "modern new",

    "build": "modern build && npm run build-storybook --output-dir ./dist/static/storybook",
    "deploy": "modern deploy && npm run build-storybook --output-dir ./.output/static/storybook",

    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",

    "lint": "biome check",
    "format": "biome format --write",

    "prepare": "simple-git-hooks",
    "commit": "npm run format && git add . && git commit",

    "reset": "npx rimraf node_modules ./**/node_modules",
    "upgrade": "modern upgrade"
  },

  "engines": {
    "node": ">=16.18.1"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --files-ignore-unknown=true"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "@modern-js/runtime": "2.66.0",
    "@module-federation/modern-js": "^0.11.3",
    "@radix-ui/react-toggle": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.486.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.1.0",
    "tw-animate-css": "^1.2.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@modern-js/app-tools": "2.66.0",
    "@modern-js/plugin-server": "2.66.0",
    "@modern-js/tsconfig": "2.66.0",
    "@rsbuild/core": "^1.3.4",
    "@rsbuild/plugin-node-polyfill": "^1.3.0",
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-backgrounds": "^8.6.12",
    "@storybook/addon-console": "^3.0.0",
    "@storybook/addon-designs": "^8.2.1",
    "@storybook/addon-docs": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-graphql": "^6.2.9",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-jest": "^8.6.12",
    "@storybook/addon-knobs": "^8.0.1",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-measure": "^8.6.12",
    "@storybook/addon-outline": "^8.6.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-queryparams": "^7.0.1",
    "@storybook/addon-storyshots": "^7.6.17",
    "@storybook/addon-storysource": "^8.6.12",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/addon-svelte-csf": "^4.2.0",
    "@storybook/addon-themes": "^8.6.12",
    "@storybook/addon-toolbars": "^8.6.12",
    "@storybook/addon-viewport": "^8.6.12",
    "@storybook/channels": "^8.6.12",
    "@storybook/manager-api": "^8.6.12",
    "@storybook/preview-api": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/bun": "^1.2.8",
    "@types/jest": "~29.2.4",
    "@types/node": "~18.11.9",
